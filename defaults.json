{
  "name" : "{{name}}",
  "version" : "{{version}}",
  "_vars" : {
    "errors" :{
      "500" : {
        "{{defKey}}" : "{{locale.{{currentLocale}}.500}}",
        "requestId" : "{{requestId}}",
        "status" : 500
      },
      "408" : {
        "{{defKey}}" : "{{locale.{{currentLocale}}.408}}",
        "requestId" : "{{requestId}}",
        "status" : 408
      },
      "404" : {
        "{{defKey}}" : "{{locale.{{currentLocale}}.404}}",
        "requestId" : "{{requestId}}",
        "status" : 404
      },
      "400" : {
        "{{defKey}}" : "{{locale.{{currentLocale}}.400}}",
        "requestId" : "{{requestId}}",
        "status" : 400
      },
      "405" : {
        "{{defKey}}" : "{{locale.{{currentLocale}}.405}}",
        "requestId" : "{{requestId}}",
        "status" : 405
      },
      "inval" : {
        "{{defKey}}" : "{{locale.{{currentLocale}}.invalidparams}}",
        "requestId" : "{{requestId}}",
        "status" : 400
      }
    },
    "supportEmail" : "support@yourdomain.com",
    "name" : "servername",
    "defTimeout":18000,
    "defKey" : "_",
    "version" : "0.0.1",
    "locale":{
      "en":{
        "500" : "An unknown error has been occurred. Please contact us at {{supportEmail}} with request id {{requestId}}",
        "408" : "It taking huge time to evaluate request. Time out. Aborted.",
        "404" : "Invalid request route.",
        "405" : "Method not allowed.",
        "400" : "Invalid inputs.",
        "invalidparams" : "Request parameter was invalid."
      }
    }
  },
  "_root" : {
    "$" : {
      "name" : "{{name}}",
      "version" : "{{version}}"
    }
  }
}
